{
    "name": "packagix/packagix",
    "description": "Packagix",
    "type": "library",

    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "symfony/process": "*"

    },
    "config": {
        "platform": {
            "php": "7.1.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },

    "bin": [
        "packagix"
    ],
    "autoload": {
        "classmap": [
            "src/"
        ]
    }
}
